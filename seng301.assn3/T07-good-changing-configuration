construct(5, 10, 25, 100; 3; 10; 10; 10)
configure("A", "B", "C"; 5, 10, 25)
load(1, 1, 2, 0; 1, 1, 1)
configure("Coke", "water", "stuff"; 250, 250, 205)
press(0)
extract()
CHECK_DELIVERY(0)
insert(100)
insert(100)
insert(100)
press(0)
extract()
CHECK_DELIVERY(50, "A")
unload()
CHECK_TEARDOWN(315; 0; "B", "C")
load(1, 1, 2, 0; 1, 1, 1)
insert(100)
insert(100)
insert(100)
press(0)
extract()
CHECK_DELIVERY(50, "Coke")
unload()
CHECK_TEARDOWN(315; 0; "water", "stuff")
