construct(5, 10, 25, 100; 1; 10; 10; 10)
configure("stuff"; 140)
load(0, 5, 1, 1; 1)
insert(100)
insert(100)
insert(100)
press(0)
extract()
CHECK_DELIVERY(155, "stuff")
unload()
CHECK_TEARDOWN(320; 0)
load(10, 10, 10, 10; 1)
insert(25)
insert(100)
insert(10)
press(0)
extract()
CHECK_DELIVERY(0, "stuff")
unload()
CHECK_TEARDOWN(1400; 135)
